<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd" >
<sqlMap namespace="product">

	<select id="selectAll" resultClass="com.kosta.k153p2.product.dto.ItemInfo">
		select * from ITEMINFO
	</select>

	<select id="selMenu" resultClass="com.kosta.k153p2.product.dto.ItemInfo" parameterClass="int" >
		select * from ITEMINFO where ITEM_TYPE=#value#
	</select>
</sqlMap>
